@{

}
@section Styles
{
    <link rel="stylesheet" href="~/css/Transactions.css" />
}


<h1>Depositar dinero</h1>
<div id="transaction" class="div_form">
    <select v-model="IdSelectedAccount">
        <option value="-1" selected disabled>Selecciona una cuenta</option>
        <option v-for="item in Accounts" :value="item.Id">{{ item.Name }}</option>
    </select>
    <input type="number" min="0.0" max="50000" v-model="Amount" />
    <button v-on:click="Doit()">Depositar</button> 
    
</div>
@section Scripts
{
    <script src="~/Vue/apps/Deposit.js"></script>
}
